{
  "name": "platzi-optimizacion-web",
  "version": "0.1.0",
  "description": "Proyecto del curso de Optimizacion Web de Platzi",
  "author": "@jonalvarezz",
  "type": "module",
  "scripts": {
    "start": "npm run build; npm run serve",
    "dev": "webpack-dev-server --config webpack.config.dev.js",
    "build": "webpack --config webpack.config.cjs",
    "serve": "serve . -p 3000",
    "build:node": "npm run build; node export.js",
    "netlify": "npm run build && node export.js"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.14.198",
    "@types/lozad": "^1.16.2",
    "css-loader": "^5.2.6",
    "husky": "^4.2.3",
    "hyperscript": "^2.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.24.0",
    "prettier": "1.19.1",
    "pretty-quick": "^2.0.1",
    "style-loader": "^2.0.0",
    "webpack": "^4.46.0",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.3"
  },
  "dependencies": {
    "express": "^4.18.2",
    "lozad": "^1.16.0",
    "modal-video": "^2.4.8",
    "node": "^20.6.0",
    "serve": "^14.2.1"
  }
}
